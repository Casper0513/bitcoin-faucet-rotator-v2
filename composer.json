{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "^5.4",
        "laravel/passport": "^3.0",
        "cviebrock/eloquent-sluggable": "4.2.*",
        "yajra/laravel-datatables-oracle": "7.*",
        "barryvdh/laravel-ide-helper": "2.3.*",
        "infyomlabs/laravel-generator": "5.4.x-dev",
        "laravelcollective/html": "^5.4.0",
        "infyomlabs/adminlte-templates": "5.4.x-dev",
        "infyomlabs/swagger-generator": "dev-master",
        "jlapp/swaggervel": "dev-master",
        "doctrine/dbal": "^2.5",
        "way/generators": "~3.0",
        "roumen/sitemap": "2.6.*",
        "abraham/twitteroauth": "^0.7.3",
        "roumen/feed": "^2.10",
        "mews/purifier": "~2.0",
        "laracasts/generators": "~1.0",
        "league/csv": "^8.1",
        "michelecurletta/laravel-schedule-overview": "^1.0",
        "predis/predis": "1.1.*",
        "santigarcor/laratrust": "4.*",
        "league/fractal": "^0.16.0",
        "phpoffice/phpexcel": "^1.8",
        "squareboat/sneaker": "^2.0",
        "spatie/laravel-activitylog": "^1.15",
        "matthewbdaly/laravel-etag-middleware": "^1.0",
        "bepsvpt/secure-headers": "^3.0",
        "psr/http-message": "^1.0",
        "dougsisk/laravel-block-referral-spam": "^1.0",
        "artesaos/seotools": "^0.10.0",
        "greggilbert/recaptcha": "^2.2",
        "creativeorange/gravatar": "^1.0",
        "htmlmin/htmlmin": "^4.5",
        "spatie/laravel-analytics": "^3.0",
        "lord/laroute": "^2.4",
        "fideloper/proxy": "~3.3",
        "sentry/sentry-laravel": "^0.8.0",
        "optimus/api-consumer": "^0.2.3",
        "asm89/stack-cors": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.6",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.2",
        "symfony/css-selector": "3.4.x-dev",
        "symfony/dom-crawler": "3.4.x-dev",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "^3.0",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "app"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:models",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
