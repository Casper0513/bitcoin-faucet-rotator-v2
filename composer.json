{
    "name": "rattfieldnz/bitcoin-faucet-rotator-v2",
    "description": "A database-driven bitcoin faucet rotator built with Laravel version 5.5.*.",
    "keywords": [
      "laravel56",
      "laravel",
      "laravel-framework",
      "bitcoin",
      "bitcoin-faucet-rotator",
      "php71",
      "mysql",
      "linux-server"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.7.*",
        "laravel/passport": "~7.0",
        "cviebrock/eloquent-sluggable": "4.6.*",
        "yajra/laravel-datatables-oracle": "8.*",
        "laravelcollective/html": "^5.7",
        "doctrine/dbal": "^2.9",
        "laravelium/sitemap": "^3.0",
        "abraham/twitteroauth": "^0.9",
        "laravelium/feed": "^3.0",
        "mews/purifier": "~2.0",
        "league/csv": "^9.1",
        "predis/predis": "1.1.*",
        "santigarcor/laratrust": "^5.0",
        "league/fractal": "^0.17.0",
        "maatwebsite/excel": "~3.1",
        "phpoffice/phpspreadsheet": "^1.6",
        "spatie/laravel-activitylog": "^3.1",
        "bepsvpt/secure-headers": "^5.3",
        "psr/http-message": "^1.0",
        "artesaos/seotools": "^0.12",
        "greggilbert/recaptcha": "^2.2",
        "creativeorange/gravatar": "^1.0",
        "htmlmin/htmlmin": "^5.7",
        "spatie/laravel-analytics": "^3.6",
        "lord/laroute": "^2.4",
        "fideloper/proxy": "~4.0",
        "sentry/sentry-laravel": "^0.11",
        "optimus/api-consumer": "^0.2.3",
        "snowfire/beautymail": "~1.0.12",
        "phpseclib/phpseclib": "~2.0",
        "barryvdh/laravel-cors": "^0.11",
        "laracasts/flash": "^3.0",
        "symfony/class-loader": "^3.4",
        "infyomlabs/laravel-generator": "5.7.x-dev",
        "infyomlabs/adminlte-templates": "5.7.x-dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.8",
        "mockery/mockery": "^1.1",
        "phpunit/phpunit": "~7.3",
        "symfony/css-selector": "^4.1",
        "symfony/dom-crawler": "^4.1",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "^3.3",
        "filp/whoops": "~2.2",
        "barryvdh/laravel-debugbar": "^3.1",
        "jaybizzle/laravel-migrations-organiser": "4.*",
        "barryvdh/laravel-ide-helper": "2.5.*",
        "laracasts/generators": "~1.1",
        "laravel/tinker": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "app"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:models",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
